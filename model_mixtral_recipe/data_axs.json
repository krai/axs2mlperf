{
    "_parent_entries": [ [ "^", "byname", "shell" ] ],
    "_producer_rules": [
        [ [ "downloaded", "checkpoint_tokenizer_files", "model_name=mistral" ], [[ "run" ]] ]
    ],

    "model_name": "mixtral-8x7b",

    "model_checkpoint_path": [ "^^", "execute", [[
        [ "get_kernel" ],
        [ "byquery", [  [ "^^", "substitute",[ "downloaded,checkpoint,model_name=#{model_name}#"]] ]],
        [ "get_path"]
        ]]
     ],
     "this_entry_path": [ "^^", "get_path", "" ],

     "shell_cmd_with_subs": "cp #{this_entry_path}#/tokenizer* #{model_checkpoint_path}#"

    
}
