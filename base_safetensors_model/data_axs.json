{
    "desired_python_version": "3.11",
    "safetensors_query": [ "python_package", "package_name=safetensors", ["desired_python_version", [ "^^", "get", "desired_python_version" ]] ],
    "torch_query": [ "python_package", "package_name=torch", ["desired_python_version", [ "^^", "get", "desired_python_version" ]] ],
    "numpy_query": [ "python_package", "package_name=numpy", ["desired_python_version", [ "^^", "get", "desired_python_version" ]] ],

    "_BEFORE_CODE_LOADING": [ "^^", "execute", [[
        [ "get_kernel" ],
        [ "byquery", [[ "^^", "get", "safetensors_query" ]] ],
        [ "use" ],
	[],
        [ "get_kernel" ],
        [ "byquery", [[ "^^", "get", "torch_query" ]] ],
        [ "use" ],
	[],
        [ "get_kernel" ],
        [ "byquery", [[ "^^", "get", "numpy_query" ]] ],
        [ "use" ]
    ]]],

    "dtypes_used": [ "^^", "get_dtypes_used" ]
}
